@using System.Configuration
<!--minifier-->
<link rel="stylesheet" href="@Url.Content("~/Scripts/OpenLayers-2.11/theme/default/style.css?")" type="text/css"/>
<script type="text/javascript" src="@Url.Content("~/Scripts/OpenLayers-2.11/OpenLayers.js?")"> </script>
@{
    string googleMap = ConfigurationManager.AppSettings["GOOGLEMAP"];
    string vietMap = ConfigurationManager.AppSettings["VIETMAPTILED"];
    if (string.IsNullOrEmpty(googleMap)
        && string.IsNullOrEmpty(vietMap))
    {
        vietMap = "OK";
    }
    if (!string.IsNullOrEmpty(vietMap))
    {<script>var VIETMAP_TILED = 1;</script>}
    if (!string.IsNullOrEmpty(googleMap))
    {
        <script>var GOOGLE_MAP = 1;</script>
        <script type="text/javascript" src="http://maps-api-ssl.google.com/maps/api/js?key=AIzaSyCrM6qZ9jEGaj21G2kzZQ_iTe5_KIkZJ4M&libraries=places"> </script>
    }
}